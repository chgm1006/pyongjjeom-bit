<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pyongjjeom.notice.dao.NoticeMapper">

	<insert id="insertData" parameterType="Notice">
		insert into test_notice
		(title,
		person, password, issue,regidate,updatedate)
		values
		(#{title},
		#{person},
		#{password},#{issue},now(),
		now())
		
	
	</insert>

	<select id="applyData" resultType="Notice">

		SELECT * FROM forcebewithyou.test_notice;
		

	</select>



</mapper>